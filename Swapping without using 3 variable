#include<stdio.h>
main()
{
	int a,b,c;
	printf("Enter the first number\t");
	scanf("%d",&a);
	printf("Enter the seconf number");
	scanf("%d",&b);
	getch();
	printf("Values before swapping of a is %d and b is %d\n",a,b);
	a=a+b;
	b=a-b;
	a=a-b;
	printf("Values after swapping of a is %d and b is %d",a,b);
}
