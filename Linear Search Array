#include<stdio.h>
main()
{
	int arr[10];
	int i,n,count=0;
	for(i=0;i<=9;i++)
	{
		printf("Enter a number\n");
		scanf(" %d",&arr[i]);
	}
	printf("Enter a number to search\n");
	scanf(" %d",&n);
	for(i=0;i<=9;i++)
	{
		if(arr[i]==n)
		{
			printf("Number is found on %d index number\n",i+1);
			count++;
		}
	}
	if(count==0)
	printf("Number not found\n");
} 
